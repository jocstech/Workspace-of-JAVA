package heapData;

public class HeapSystem {

	private static MaxHeapData<Integer> heap;
	public static void main(String[] args) 
	{
		Comparable[] unsortedArray = {12,34,23,56,23,89,5,33,2,75,13,45,67,43,56,34,78,9,24};
		int ArraySize = unsortedArray.length;
		printArray(unsortedArray);
		
		heap = new MaxHeapData<Integer>((Integer[]) unsortedArray);
		
		
		
	}
	
	
	
	public static void printArray(Comparable<Integer> Array[])
	{
		for(Comparable<Integer> i:Array)
		{
			System.out.print(i+" ");
		}
		    System.out.print("\n");
	}

}
