package algorithm_studies;

import java.awt.*;
import javax.swing.*;

public class SortingGE extends JFrame {
	
	private JPanel window,display;
	private JTextField d1,d2,d3,d4,d5,d6,d7,d8,d9,d10;
	private JButton sortBtn;
	
	SortingGE()
	{
		window = new JPanel(new BorderLayout());
		display = new JPanel(new GridLayout(1,10));
		d1= new JTextField();
		d2= new JTextField();
		d3= new JTextField();
		d4= new JTextField();
		d5= new JTextField();
		d6= new JTextField();
		d7= new JTextField();
		d8= new JTextField();
		d9= new JTextField();
		d10= new JTextField();
		
		display.add(d1);
		display.add(d2);
		display.add(d3);
		display.add(d4);
		display.add(d5);
		display.add(d6);
		display.add(d7);
		display.add(d8);
		display.add(d9);
		display.add(d10);
		
		window.add(display,BorderLayout.NORTH);
		
		
		this.setContentPane(window);
		this.setSize(600, 400);
		this.setVisible(true);
		this.setLocationByPlatform(true);
		
	}

	public static void main(String[] args) 
	{
		new SortingGE();
	}

}
